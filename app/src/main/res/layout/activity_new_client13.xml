<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    >

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="86dp"
        android:layout_gravity="center"
        android:background="@drawable/logo_add"
        android:contentDescription="logo"
        android:scaleType="center" />


    <TextView
        android:id="@+id/textView19"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:inputType="text"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:text="19. What was the date of your last HIV test?"
        android:textSize="18sp"
        />

    <DatePicker
        android:id="@+id/datePicker1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:headerBackground="@android:color/holo_blue_dark"
        app:layout_constraintTop_toBottomOf="@+id/textView19"
        tools:layout_editor_absoluteX="10dp" />

    <RelativeLayout
        android:id="@+id/relativeLayoutpage3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        >

        <Button
            android:id="@+id/buttonclientback"
            android:layout_width="130dp"
            android:layout_height="106dp"
            android:background="@drawable/addclientback"
            android:layout_alignParentLeft="true"
            android:onClick="onClickBack"
            />

        <Button
            android:id="@+id/buttonclientnext"
            android:layout_width="130dp"
            android:layout_height="106dp"
            android:background="@drawable/addclientnext"
            android:layout_alignParentRight="true"
            android:onClick="onClickNext"
            />

    </RelativeLayout>





</LinearLayout>
